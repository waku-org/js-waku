  },
  "scripts": {
    "start": "cra-webpack-rewired start",
    "build": "run-s build:*",
    "build:react": "cra-webpack-rewired build",
    "eject": "cra-webpack-rewired eject",
    "fix": "run-s fix:*",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts --ext .tsx",
